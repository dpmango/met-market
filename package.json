{
  "name": "met-market",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "lint": "bash bin/lint.sh",
    "lint:js:fix": "eslint './src/**/*.{js,jsx}' --fix",
    "lint:css:fix": "stylelint './src/**/*.{css,less,scss,sass}' --fix",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "deploy": "craco build && cp build/index.html build/200.html && surge --project ./build --domain met-market.surge.sh",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,scss}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.3.0",
    "axios": "^0.21.4",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "mobx-utils": "^6.0.4",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-spinners": "^0.11.0",
    "react-table": "^7.7.0",
    "retry-axios": "^2.6.0"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.26.0",
    "husky": "4",
    "lint-staged": "^11.1.2",
    "node-sass": "5",
    "prettier": "^2.4.1",
    "raw-loader": "^4.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.21.0",
    "typescript": "^4.4.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
